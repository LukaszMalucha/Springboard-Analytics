{"remainingRequest":"C:\\Workshop\\springboard\\frontend\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Workshop\\springboard\\frontend\\src\\views\\FastestBsc.vue?vue&type=script&lang=js&","dependencies":[{"path":"C:\\Workshop\\springboard\\frontend\\src\\views\\FastestBsc.vue","mtime":1576941496644},{"path":"C:\\Workshop\\springboard\\frontend\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Workshop\\springboard\\frontend\\node_modules\\thread-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Workshop\\springboard\\frontend\\node_modules\\babel-loader\\lib\\index.js","mtime":499162500000},{"path":"C:\\Workshop\\springboard\\frontend\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Workshop\\springboard\\frontend\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["FastestBsc.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2DA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"FastestBsc.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n<div class=\"row plain-element\">\r\n  <RowHeader/>\r\n  <div class=\"row banner\">\r\n    <div class=\"col-xs-0 col-sm-0 col-md-3 col-lg-2 text-left plain-element\">\r\n      <img src=\"https://springboard-analytics.s3-eu-west-1.amazonaws.com/static/img/bachelor-banner.jpg\" class=\"img responsive img-banner\">\r\n    </div>\r\n    <div class=\"col-xs-12 col-sm-12 col-md-8 col-lg-8 plain-element\">\r\n      <div class=\"row summary\">\r\n        <div class=\"box\">\r\n          <h5>The Fastest Bachelor Degrees</h5>\r\n          <h6>The traditional bachelor’s degree takes at least four years to complete.\r\n          Springboard offers some accelerated college programs that will allow you to complete an entire bachelor’s degree in far less time.\r\n          </h6>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div class=\"dashboard-cards\">\r\n    <div class=\"row\"></div>\r\n    <div class=\"row row-cards\">\r\n      <div class=\"col-md-10 col-lg-10 plain-element\"></div>\r\n      <div class=\"col-md-2 col-lg-2 plain-element\">\r\n        <div class=\"search-wrapper\">\r\n          <label> Search Course:</label>\r\n          <input type=\"text\" v-model=\"search\"/>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"row row-cards\">\r\n       <table id=\"courseTable\">\r\n        <thead>\r\n        <tr>\r\n          <th onclick=\"sortTable(0)\">Title</th>\r\n          <th class=\"text-center\" onclick=\"sortTable(1)\">Provider</th>\r\n          <th class=\"text-center\" onclick=\"sortTable(2)\">Duration</th>\r\n          <th class=\"text-center\"  onclick=\"sortTable(4)\">Mode</th>\r\n          <th class=\"text-center\" onclick=\"sortTable(5)\">Deadline</th>\r\n          <th class=\"text-center\" onclick=\"sortTable(6)\">Website</th>\r\n        </tr>\r\n        </thead>\r\n        <tbody>\r\n        <tr v-for=\"course in filteredList\" :key=\"course.pk\">\r\n          <td class=\"col-course\">{{ course[1]}}</td>\r\n          <td class=\"col-course text-center\">{{ course[2] }}</td>\r\n          <td class=\"text-center\">{{ course[15]}} Days</td>\r\n          <td class=\"text-center\">{{ course[5] }}</td>\r\n          <td class=\"text-center\">{{ course[6] }}</td>\r\n          <td class=\"text-center\"><a target=\"_blank\" :href=\"course[11]\">Website</a></td>\r\n        </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport RowHeader from \"@/components/RowHeader.vue\";\r\nimport { apiService } from \"@/common/api.service.js\";\r\n\r\nlet endpoint = \"api/fastest-bachelor\";\r\n\r\nexport default {\r\n  name: \"FastestBachelor\",\r\n  components: {\r\n    RowHeader,\r\n  },\r\n  data() {\r\n    return {\r\n      search: \"\",\r\n      courseList: []\r\n    }\r\n  },\r\n  methods: {\r\n    getCoursesData() {\r\n      apiService(endpoint)\r\n        .then(data => {\r\n\r\n          this.courseList = data\r\n        })\r\n      }\r\n   },\r\n   filters: {\r\n      truncate (value, limit) {\r\n          if (value.length > limit) {\r\n              value = value.substring(0, limit) + \"...\";\r\n          }\r\n          return value\r\n      }\r\n  },\r\n  computed: {\r\n//  Search courses function\r\n    filteredList() {\r\n      return this.courseList.filter(course => {\r\n        return course[1].toLowerCase().includes(this.search.toLowerCase()) ||\r\n               course[2].toLowerCase().includes(this.search.toLowerCase())\r\n      })\r\n    }\r\n  },\r\n  created() {\r\n    this.getCoursesData()\r\n    document.title = \"The Fastest Bachelor Degree\";\r\n  }\r\n}\r\n</script>"]}]}